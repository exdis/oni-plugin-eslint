    "description": "An eslint plugin for Oni",
    "main": "lib/index.js",
    "repository": "git@github.com:exdis/oni-plugin-eslint.git",
    "author": "exdis",
    "license": "MIT",
    "engines": {
        "oni": "^0.3.6"
    },
    "oni": {
